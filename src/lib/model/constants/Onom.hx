	/**
	 *  get the character, if not use the default
	 *  @param value -
	 */
	public function getChar(value){
		var pointsArray = char_not;
		if(mono.exists(value)){
			pointsArray = mono.get(value);
		}
		// make sure the values are not directly mapped back to
		// this file copy all points and create a new arr.
		var temp = [];
		for ( i in 0 ... pointsArray.length ) {
			var oldP = pointsArray[i];
			var newP = new Point(oldP.x, oldP.y);
			temp.push(newP);
		}
		return temp;
	}

	public function getAvailable(){
		var str = '';
		for (value in mono.keys()){
			str += value;
		}
		return str;
	}


